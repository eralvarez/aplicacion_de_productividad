{
  "name": "webapp-starter",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc --project tsconfig.json",
    "postxbuild": "tspath -f",
    "startx": "node dist/src/server.js",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run start\"",
    "start:prod": "npm run start",
    "db": "npm run db:sync && npm run db:seed",
    "db:dev": "npm run db:drop && npm run db",
    "db:sync": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:drop": "node ./src/db/helpers/drop_db.js",
    "db:clean": "node ./src/db/helpers/clean_db.js",
    "test:e2e": "mocha --config e2e.mocharc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eralvarez/webapp-starter.git"
  },
  "author": "Erick Alvarez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eralvarez/webapp-starter/issues"
  },
  "homepage": "https://github.com/eralvarez/webapp-starter#readme",
  "dependencies": {
    "@types/cors": "~2.8.6",
    "@types/express": "~4.17.7",
    "@types/joi": "~14.3.4",
    "@types/md5": "~2.2.0",
    "@types/morgan": "~1.9.1",
    "@types/node": "~14.0.24",
    "@types/nodemailer": "~6.4.0",
    "@types/pbkdf2": "~3.1.0",
    "@types/redis": "~2.8.25",
    "body-parser": "~1.18.3",
    "chalk": "~2.4.1",
    "cors": "~2.8.4",
    "crypto": "~1.0.1",
    "express": "~4.16.3",
    "joi": "~14.3.1",
    "jsonwebtoken": "~8.3.0",
    "knex": "~0.21.2",
    "knexnest": "~1.0.0",
    "lodash": "~4.17.10",
    "md5": "~2.3.0",
    "mocha": "^8.1.1",
    "module-alias": "~2.2.2",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "multer": "~1.4.1",
    "node-cron": "~2.0.3",
    "nodemailer": "~5.1.1",
    "pbkdf2": "~3.1.1",
    "pg": "~7.4.3",
    "pg-hstore": "~2.3.2",
    "redis": "~3.0.2"
  },
  "devDependencies": {
    "browser-sync": "~2.26.3",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "dotenv": "~6.0.0",
    "faker": "~4.1.0",
    "knex-cleaner": "~1.3.0",
    "minimist": "~1.2.5",
    "nodemon": "~2.0.4",
    "request": "~2.88.0",
    "sinon": "~9.0.2",
    "ts-node": "~8.10.2",
    "tsconfig-paths": "~3.9.0",
    "tspath": "~1.3.7",
    "typescript": "~3.9.7"
  }
}
