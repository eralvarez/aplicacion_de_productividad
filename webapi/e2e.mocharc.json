{
    "diff": true,
    "extension": [
        "js"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": 75,
    "timeout": 2000,
    "ui": "bdd",
    "spec": "e2e/**/*.spec.js",
    "watch-files": [
        "src/**/*.js",
        "e2e/**/*.js"
    ],
    "watch-ignore": [
        "lib/vendor"
    ]
}